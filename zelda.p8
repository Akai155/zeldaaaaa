pico-8 cartridge // http://www.pico-8.com
version 16
__lua__


function _init()
	create_player()
	init_cam()
end

function _update()
	flayer_movement()
	chek_flag(flag,x,y)
	update_camera()
end

function _draw()
	cls(15)
	draw_map()
	draw_player()
	draw_ui()
end
-->8
--map--

function draw_map()
	map(0,0,0,0,125,64)
end

function chek_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function init_cam()
	camx,camy=0,16
end

function update_camera()
	secx= flr(p.x/16)*16
	secy= flr(p.y/16)*16
	
	destx=secx*8
	desty=secy*8
	
	diffx=destx-camx
	diffy=desty-camy
	
	diffx/=4
	diffy/=4
	
	camx+=diffx
	camy+=diffy
	
	camera(camx,camy)
end


function old_camera()
	camx=mid(0,p.x-7.5,68)
	camy=mid(0,p.y-7.5,36)
	camera(camx*8,camy*8)
end

function next_title(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end

function pick_up_key(x,y)
	next_title(x,y)
	p.keys+=1
	sfx(1)
end

function open_door(x,y)
	if p.keys>0 then
		next_title(x,y)
		p.keys-=1
 	sfx(3)
 	end
end
-->8
--player

function create_player()
	p={x=6,y=20,
	ox=0,oy=0,
	sox=0,soy=0,
	anim_t=0,
	sprite=64,
	keys=0,
	flip=false
	}
	
end

function flayer_movement()
	newx=p.x
	newy=p.y
	if p.anim_t==0 then
 	if btn(➡️) then
 	 newx+=1
 	 newox=-8
 	 newoy=0
 	 p.flip=false
 	elseif btn(⬅️) then
 	 newx-=1
 	 newox=8
 	 newoy=0
 	 p.flip=true
 	elseif btn(⬆️) then
 	 newy-=1
 	 newox=0
 	 newoy=8
 	elseif btn(⬇️) then
 	 newy+=1
 	 newox=0
 	 newoy=-8
 	end
	end
	 
	intrect(newx,newy) 
	 
	if not chek_flag(0,newx,newy)
	and (p.x!=newx or p.y!=newy) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
		p.sox=newox
		p.soy=newoy
		p.anim_t=1
	end
	
	--animation
	p.anim_t=max(p.anim_t-0.125,0)
	p.ox=p.sox*p.anim_t
	p.oy=p.soy*p.anim_t
	
	if p.anim_t>=0.5 then
		p.sprite=65
	else
		p.sprite=64
	end	
	
end

function intrect(x,y)
	if chek_flag(1,x,y) then
		pick_up_key(x,y)
	elseif chek_flag(2,x,y) then
		open_door(x,y)
	end
end

function draw_player()
	--spr(p.sprite,p.x*8,p.y*8)
	spr(p.sprite,
	p.x*8+p.ox,p.y*8+p.oy,
	1,1,
	p.flip)
end
-->8
-- ui

function draw_ui()
	if p.keys>0 then
		camera()
		
		palt(0,false)
		palt(12,true)
		spr(53,3,2.5)
		palt()
		print_outline("x"..p.keys, 13,3,7)
	end
end

function print_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end
